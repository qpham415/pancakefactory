<h1>All Products</h1>
<table border="1" style="width:100%">
  <tr>
    <th>Shopify ID</th>
    <th>first_name</th>
    <th>last_name</th>
    <th>email:</th>
    <th>default_address_address1</th>
    <th>default_address_city</th>
  <tr>

  <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.shopify_id %></td>
      <td><%= customer.first_name %></td>
      <td><%= customer.last_name %></td>
      <td><%= customer.email %></td>
      <td><%= customer.default_address_address1 %></td>
      <td><%= customer.default_address_city %></td>
      <td><%= link_to 'Show', customer_path(customer) %></td>
      <td><%= link_to 'Edit', edit_customer_path(customer) %></td>
      <td><%= link_to 'Destroy', customer_path(customer), method: :delete, data: { confirm:'Are you sure?'} %></td>
    </tr>
  <% end %>
</table>
<br><br><br>
<table border="1" style="width:100%">
  <% @shopify_customers.each do |t| %>
    <tr>
      <td><%= t.id %></td>
      <td><%= t.first_name %></td>
      <td><%= t.last_name %></td>
      <td><%= t.default_address.city %></td>
      <td><%= t.default_address.address1 %></td>
    </tr>
  <% end %>
</table>
